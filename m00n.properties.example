# M00n Reporter Configuration
# Copy this file to m00n.properties and fill in your values

# Enable/disable reporter (set to false for local development)
m00n.enabled=true

# Required: M00n Report ingest server URL
m00n.serverUrl=http://localhost:4001

# Required: Project API key (get from M00n Report dashboard)
m00n.apiKey=m00n_your_api_key_here

# Optional: Run/launch name
m00n.launch=Playwright Java Tests

# Optional: Tags (comma-separated)
m00n.tags=smoke,regression

# Optional: Debug mode (true/false)
m00n.debug=false

# Optional: HTTP timeout in milliseconds
m00n.timeout=30000

# Optional: Max retry attempts
m00n.maxRetries=3

# Optional: Custom attributes (prefix with m00n.attribute.)
m00n.attribute.environment=staging
m00n.attribute.browser=chromium
m00n.attribute.ci=false
